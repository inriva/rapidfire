<tr id= "survey_<%= survey.id %>">
  <td>
    <% if can_administer? %>
      <%= link_to survey.name, survey_questions_path(survey) %>
    <% else %>
      <%= survey.name %>
    <% end %>
  </td>
  <td>
    <div class="btn-group">
      <%= link_to t('surveys.answer_questions'), new_survey_attempt_path(survey), :class => 'btn btn-primary'  %>
      <% if can_administer? %>
        <%= link_to t('surveys.results'), [:results, survey], :class => 'btn btn-primary' %>
        <%= link_to t('common.delete'), survey, method: :delete, :class => 'btn btn-danger' %>
      <% end %>
    </div>
  </td>
</tr>